{
  "name": "cordis",
  "description": "Meta-Framework for Modern JavaScript Applications",
  "version": "4.0.0-alpha.0",
  "type": "module",
  "bin": "lib/bin/cordis.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./cli": {
      "types": "./lib/cli.d.ts",
      "default": "./lib/cli.js"
    },
    "./group": {
      "types": "./lib/plugins/group.d.ts",
      "default": "./lib/plugins/group.js"
    },
    "./import": {
      "types": "./lib/plugins/import.d.ts",
      "default": "./lib/plugins/import.js"
    },
    "./loader": {
      "types": "./lib/plugins/loader.d.ts",
      "default": "./lib/plugins/loader.js"
    },
    "./worker": {
      "types": "./lib/worker/index.d.ts",
      "default": "./lib/worker/index.js"
    },
    "./main": "./lib/worker/main.js",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/cordis.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/cordiverse/cordis/issues"
  },
  "homepage": "https://github.com/cordiverse/cordis",
  "keywords": [
    "cordis",
    "effect",
    "framework"
  ],
  "cordis": {
    "exports": {
      ".": null,
      "./group": {
        "description": "Group Plugins"
      },
      "./import": {
        "description": "Import Configurations"
      }
    }
  },
  "dependencies": {
    "@cordisjs/core": "4.0.0-alpha.0",
    "@cordisjs/loader": "^1.0.0-alpha.0",
    "cac": "^6.7.14",
    "cosmokit": "^1.6.3",
    "kleur": "^4.1.5",
    "reggol": "^1.7.1"
  }
}
