{
  "name": "cordis",
  "description": "Meta-Framework for Modern JavaScript Applications",
  "version": "3.13.1",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "bin": "lib/bin/cordis.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./cli": {
      "default": "./lib/cli.mjs",
      "types": "./lib/cli.d.ts"
    },
    "./group": {
      "default": "./lib/group.mjs",
      "types": "./lib/group.d.ts"
    },
    "./worker": {
      "default": "./lib/worker/index.mjs",
      "types": "./lib/worker/index.d.ts"
    },
    "./worker/main": "./lib/worker/main.mjs",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/cordis.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/cordiverse/cordis/issues"
  },
  "homepage": "https://github.com/cordiverse/cordis",
  "keywords": [
    "cordis",
    "effect",
    "framework"
  ],
  "dependencies": {
    "@cordisjs/core": "3.13.1",
    "@cordisjs/loader": "^0.8.1",
    "@cordisjs/logger": "^0.3.2",
    "@cordisjs/timer": "^0.3.2",
    "cac": "^6.7.14",
    "cosmokit": "^1.5.2",
    "kleur": "^4.1.5",
    "reggol": "^1.6.3",
    "schemastery": "^3.14.3"
  }
}
